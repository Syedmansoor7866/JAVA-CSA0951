public class Trans_Matrix {
    public static void main(String[] args){
        int rows=3,cols=3;
        int a[][]={{1,2,3},{4,5,6},{7,8,9}};
        int c[][]=new int[cols][rows];
        for(int i=0;i<rows;i++){
            for(int j=0;j<cols;j++){
                c[i][j]=a[i][j];
            }
        }
        for(int i=0;i<rows;i++){
            for(int j=0;j<cols;j++){
                System.out.print(c[i][j]+" ");
            }
            System.out.println();
        }
    }
}
