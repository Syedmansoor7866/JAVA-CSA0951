public class Fibonacci {
    public static void main(String[] args){
        int n1=0,n2=1,n3,num=5,i;
        System.out.println(n1);
        System.out.println(n2);
        for(i=2;i<num;i++){
            n3=n1+n2;
            System.out.println(n3);
            n1=n2;
            n2=n3;
        }
    }
}
